{
    "extends": ["airbnb"],
		"parserOptions": {
			"ecmaVersion": 2020
		},
		"env": {
			"browser": true,
			"node": true,
			"jasmine": true
		},
    "rules": {
        // "prettier/prettier": ["error"],
				"react/prop-types": "off",
        "no-anonymous-exports-page-templates": 0,
        "limited-exports-page-templates": 0,
        "import/prefer-default-export": 0,
        "indent": ["error", 2],
        "no-tabs": 0,
				"no-param-reassign": 0,
				"react/jsx-props-no-spreading": 0,
				"react/no-array-index-key":0    
		},
    "settings": {
        "import/resolver": {
            "node": {
                "paths": ["src"]
            }
        }
    }
}
